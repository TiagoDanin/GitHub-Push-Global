{
	"name": "github-push-global",
	"main": "index.js",
	"bin": {
		"github-push-global": "index.js"
	},
	"preferGlobal": true,
	"version": "1.1.0",
	"description": "Commit in all repositories with a command",
	"author": {
		"email": "TiagoDanin@outlook.com",
		"name": "Tiago Danin",
		"url": "https://TiagoDanin.github.io"
	},
	"license": "MIT",
	"keywords": [
		"git",
		"github",
		"github-app",
		"github-tools",
		"global",
		"push"
	],
	"scripts": {
		"start": "node index.js",
		"test": "xo"
	},
	"engines": {
		"node": ">=8"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TiagoDanin/GitHub-Push-Global.git"
	},
	"homepage": "https://TiagoDanin.github.io/GitHub-Push-Global",
	"bugs": {
		"url": "https://github.com/TiagoDanin/GitHub-Push-Global/issues"
	},
	"github": {
		"name": "GitHub-Push-Global",
		"owner": "TiagoDanin"
	},
	"files": [
		"LICENSE",
		"README.md",
		"index.js",
		"package.json"
	],
	"dependencies": {
		"@octokit/rest": "16.25.4",
		"axios": "^0.18.1",
		"js-yaml": "3.13.1",
		"meow": "^5.0.0",
		"update-notifier": "^3.0.1"
	},
	"devDependencies": {
		"xo": "^0.24.0"
	},
	"xo": {
		"rules": {
			"quote-props": "warn",
			"eqeqeq": "warn",
			"no-await-in-loop": "warn",
			"array-callback-return": "warn",
			"require-atomic-updates": "warn",
			"no-else-return": "off"
		},
		"semicolon": false
	}
}
